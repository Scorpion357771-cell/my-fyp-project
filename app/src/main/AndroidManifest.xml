<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fyp.diseasedetector">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiseaseDetector">
        <activity android:name=".doctorprofile"></activity>
        <activity android:name=".Doctor.DoctorSignup" />
        <activity android:name=".Doctor.DoctorLogin" />
        <activity android:name=".LoginSelection" />
        <activity android:name=".UserPorfile" />
        <activity android:name=".Patient.PatientProfile" />
        <activity android:name=".Common.LoginSignup.PatientStartUpScreen" />
        <activity android:name=".Common.LoginSignup.PatientStart" />
        <activity android:name=".Patient.AllCategories" />
        <activity android:name=".Common.Onboarding" />
        <activity android:name=".Patient.PatientDashboard" />
        <activity android:name=".Common.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>